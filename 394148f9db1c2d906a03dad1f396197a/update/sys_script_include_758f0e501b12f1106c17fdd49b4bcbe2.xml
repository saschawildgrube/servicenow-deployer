        <api_name>x_snc_deployer.GetDeploymentTaskStates</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns an object that maps state ids to their numeric values.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetDeploymentTaskStates</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetDeploymentTaskStates()
{
	var states = {};
	states.pending     		= 1000;
	states.ready       		= 2000;
	states.inprogress  		= 3000;
	states.complete    		= 10000;
	states.cancelled   		= 11000;
	states.failed      		= 12000;
	states.skipped     		= 13000;
	states.failure_ignored  = 14000;
	return states;
}]]></script>
